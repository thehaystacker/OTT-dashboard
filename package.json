{
	"name": "ott-dashboard",
	"version": "1.0.0",
	"description": "An OTT dashboard with both client and server",
	"main": "index.js",
	"author": "thehaystacker",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": "15.2.0",
		"npm": "7.0.8"
	},
	"dependencies": {
		"express": "^4.17.1"
	},
	"scripts": {
		"start": "node index.js",
		"heroku-postbuild": "cd client && yarn install && yarn run build",
		"dev": "nodemon --watch server -e ts,ejs --exec yarn start:dev",
		"start:dev": "tsc server/index.ts && node server/index.js"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"nodemon": "^2.0.7",
		"tslint": "^6.1.3",
		"typescript": "^4.2.4"
	}
}
